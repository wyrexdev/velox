{
  "name": "@velox/server",
  "version": "1.0.0",
  "description": "Ultra Modern, Ultra Modular, Ultra Fast, Ultra Secure HTTP Server with Streaming, Clustering, and Worker Thread Support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:cjs && npm run build:esm && npm run build:types",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json && node scripts/fix-esm.js",
    "build:types": "tsc -p tsconfig.types.json",
    "clean": "rimraf dist",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "start:cluster": "CLUSTER_MODE=true node dist/index.js",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "prepublishOnly": "npm run build && npm run test",
    "prepack": "npm run build",
    "postpack": "npm run clean",
    "release": "npm run build && npm publish",
    "release:beta": "npm run build && npm publish --tag beta",
    "docs": "typedoc src/index.ts --out docs",
    "benchmark": "node benchmarks/performance.js"
  },
  "keywords": [
    "http-server",
    "file-upload",
    "security",
    "clustering",
    "worker-threads",
    "streaming",
    "typescript",
    "modular",
    "performance",
    "enterprise",
    "cors",
    "rate-limiting",
    "compression",
    "middleware",
    "routing",
    "velox"
  ],
  "author": {
    "name": "VELOX Team",
    "email": "team@velox-server.dev",
    "url": "https://velox-server.dev"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/velox/velox-server.git"
  },
  "bugs": {
    "url": "https://github.com/velox/velox-server/issues"
  },
  "homepage": "https://velox-server.dev",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/velox-server"
  },
  "dependencies": {
    "mime-types": "^2.1.35",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.10",
    "tsx": "^4.6.2",
    "typedoc": "^0.25.4",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "node": ">=18.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "volta": {
    "node": "20.10.0",
    "npm": "10.2.3"
  }
}